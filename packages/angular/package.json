{
  "name": "@loona/angular",
  "version": "1.0.0",
  "description": "App State Management done with GraphQL (angular integration)",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/bundles/loona.angular.umd.js",
  "module": "build/fesm5/loona.angular.js",
  "typings": "build/loona.angular.d.ts",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/loona"
  },
  "website": "https://loonajs.com",
  "keywords": [
    "loona",
    "apollo",
    "angular",
    "graphql",
    "local",
    "flux",
    "redux",
    "state",
    "state-management"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "build": "ng-packagr -p ng-package.json",
    "clean": "rimraf build/",
    "prebuild": "yarn clean",
    "release": "yarn build && npm publish build",
    "release:canary": "yarn build && npm publish build --tag canary"
  },
  "peerDependencies": {
    "@angular/core": "^6.1.0 && ^7.0.0",
    "apollo-angular": "^1.3.0",
    "apollo-client": "^2.3.0",
    "apollo-cache": "^1.0.0",
    "apollo-link": "^1.0.0",
    "graphql": "^0.13.2 || ^14.0.0",
    "rxjs": "^6.0.0"
  },
  "dependencies": {
    "@loona/core": "1.0.0"
  },
  "devDependencies": {
    "@angular/common": "7.2.12",
    "@angular/compiler": "7.2.12",
    "@angular/compiler-cli": "7.2.12",
    "@angular/core": "7.2.12",
    "@angular/platform-browser": "7.2.12",
    "@angular/platform-browser-dynamic": "7.2.12",
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.11",
    "apollo-angular": "1.5.0",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "jest": "24.7.1",
    "ng-packagr": "4.7.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "2.6.3",
    "rxjs": "6.4.0",
    "ts-jest": "24.0.2",
    "tsickle": "0.39.1",
    "typescript": "3.2.4",
    "zone.js": "0.9.0",
    "apollo-link": "1.2.11"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.+(spec.ts)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/tests/_setup.ts"
    ]
  }
}