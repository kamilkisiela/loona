{
  "name": "@loona/angular",
  "version": "1.0.0-alpha.1",
  "description": "App State Management done with GraphQL (angular integration)",
  "main": "build/bundle.umd.js",
  "module": "build/index.js",
  "typings": "build/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/kamilkisiela/loona",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "exit 0",
    "bundle": "rollup -c rollup.config.js",
    "build": "ngc -p tsconfig.json",
    "clean": "rimraf build/",
    "prebuild": "yarn clean",
    "postbuild": "yarn bundle",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --config ../../.prettierrc --write {src,tests}/**/*.ts"
  },
  "peerDependencies": {
    "@angular/core": "^6.1.0",
    "apollo-angular": "^1.2.0",
    "apollo-client": "^2.3.0",
    "graphql": "^0.13.2",
    "rxjs": "^6.0.0"
  },
  "dependencies": {
    "@loona/core": "^1.0.0-alpha.1"
  },
  "devDependencies": {
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/compiler-cli": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@types/jest": "22.2.3",
    "apollo-angular": "1.2.0",
    "apollo-cache-inmemory": "1.2.9",
    "apollo-client": "2.4.1",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "jest": "22.4.4",
    "ng-packagr": "4.1.1",
    "prettier": "1.14.2",
    "rimraf": "2.6.2",
    "rollup": "0.61.2",
    "rxjs": "6.2.2",
    "ts-jest": "22.4.6",
    "typescript": "2.9.2",
    "zone.js": "0.8.26"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "keywords": [
    "apollo",
    "angular",
    "flux",
    "state",
    "graphql",
    "local"
  ]
}