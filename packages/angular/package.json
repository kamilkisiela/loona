{
  "name": "@loona/angular",
  "version": "1.0.0-beta.1",
  "description": "App State Management done with GraphQL (angular integration)",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/bundles/loona.angular.umd.js",
  "module": "build/fesm5/loona.angular.js",
  "typings": "build/loona.angular.d.ts",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/loona"
  },
  "website": "https://loonajs.com",
  "keywords": [
    "loona",
    "apollo",
    "angular",
    "graphql",
    "local",
    "flux",
    "redux",
    "state",
    "state-management"
  ],
  "scripts": {
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "build": "ng-packagr -p ng-package.json",
    "clean": "rimraf build/",
    "prebuild": "yarn clean",
    "deploy": "yarn build && npm publish build",
    "deploy:canary": "yarn build && npm publish build --tag canary"
  },
  "peerDependencies": {
    "@angular/core": "^6.1.0",
    "apollo-angular": "^1.2.0",
    "apollo-client": "^2.3.0",
    "apollo-cache": "^1.0.0",
    "apollo-link": "^1.0.0",
    "graphql": "^0.13.2 || ^14.0.0",
    "rxjs": "^6.0.0"
  },
  "dependencies": {
    "@loona/core": "1.0.0-beta.1"
  },
  "devDependencies": {
    "@angular/common": "6.1.7",
    "@angular/compiler": "6.1.7",
    "@angular/compiler-cli": "6.1.7",
    "@angular/core": "6.1.7",
    "@angular/platform-browser": "6.1.7",
    "@angular/platform-browser-dynamic": "6.1.7",
    "@types/graphql": "14.0.0",
    "@types/jest": "23.3.2",
    "apollo-angular": "1.3.0",
    "apollo-cache-inmemory": "1.2.10",
    "apollo-client": "2.4.2",
    "graphql": "14.0.2",
    "graphql-tag": "2.9.2",
    "jest": "23.6.0",
    "ng-packagr": "4.2.0",
    "reflect-metadata": "0.1.12",
    "rimraf": "2.6.2",
    "rxjs": "6.3.2",
    "ts-jest": "23.1.4",
    "typescript": "2.9.2",
    "zone.js": "0.8.26"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**/*.+(spec.ts)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "setupFiles": [
      "<rootDir>/tests/_setup.ts"
    ]
  }
}