{
    "name": "@loona/react",
    "version": "1.0.0-alpha.1",
    "description": "App State Management done with GraphQL (react integration)",
    "author": "Kamil Kisiela <kamil.kisiela@gmail.com>",
    "license": "MIT",
    "sideEffects": false,
    "main": "build/bundles/loona.react.umd.js",
    "module": "build/fesm5/loona.react.js",
    "typings": "build/loona.react.d.ts",
    "repository": {
        "type": "git",
        "url": "kamilkisiela/loona"
    },
    "keywords": [
        "loona",
        "apollo",
        "react",
        "graphql",
        "local",
        "flux",
        "redux",
        "state",
        "state-management"
    ],
    "scripts": {
        "test": "exit 0",
        "build": "ng-packagr -p ng-package.json",
        "clean": "rimraf build/",
        "prebuild": "yarn clean",
        "deploy": "yarn build && npm publish build"
    },
    "peerDependencies": {
        "react-apollo": "^2.1.0",
        "apollo-client": "^2.3.0",
        "graphql": "^0.13.2",
        "react": "0.14.x || 15.* || ^15.0.0 || ^16.0.0"
    },
    "dependencies": {
        "@loona/core": "^1.0.0-alpha.1"
    },
    "devDependencies": {
        "@types/jest": "22.2.3",
        "@types/react": "16.4.13",
        "@types/react-dom": "16.0.7",
        "apollo-cache-inmemory": "1.2.9",
        "apollo-client": "2.4.1",
        "graphql": "0.13.2",
        "graphql-tag": "2.9.2",
        "jest": "22.4.4",
        "ng-packagr": "4.1.1",
        "rimraf": "2.6.2",
        "react": "16.4.2",
        "react-apollo": "2.1.11",
        "react-dom": "16.4.2",
        "ts-jest": "22.4.6",
        "typescript": "2.9.2"
    },
    "jest": {
        "globals": {
            "ts-jest": {
                "tsConfigFile": "tsconfig.test.json"
            }
        },
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "testMatch": [
            "**/__tests__/*.+(ts|tsx|js)"
        ],
        "moduleFileExtensions": [
            "ts",
            "js"
        ]
    }
}